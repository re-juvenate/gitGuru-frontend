{
    "manifest_version": 3,
    "name": "GitHub Guru",
    "version": "1.0",
    "description": "Help users to get better understanding of the GitHub issues.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["*://github.com/*/issues/*"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }