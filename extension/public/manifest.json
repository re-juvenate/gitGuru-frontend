{
  "manifest_version": 3,
  "name": "GitGuru Extension",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": [
      "activeTab"
  ],
  "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": [
              "content.js"
          ]
      }
  ],
  "icons": {
      "48": "faviconTransparent.png",
      "128": "faviconTransparent.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "web_accessible_resources": [
      {
          "resources": ["assets/*","style.css"],
          "matches": ["<all_urls>"]
      }
  ]
}
